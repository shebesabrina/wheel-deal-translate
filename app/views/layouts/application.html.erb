<!DOCTYPE html>
<html>
  <head>
    <title>BikeShare</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |type, message| %>
    <%= content_tag :div, message, class: type %>
    <% end %>

    <p>Cart: <%= @cart.total_count %></p>

    <%= yield %>
  </body>
</html>
